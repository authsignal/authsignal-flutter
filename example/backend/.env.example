# Authsignal Configuration
# Copy this file to .env and update with your actual credentials
# Get your credentials from: https://portal.authsignal.com

AUTHSIGNAL_SECRET=your_secret_key_here
AUTHSIGNAL_TENANT_ID=your_tenant_id_here
AUTHSIGNAL_BASE_URL=https://api.authsignal.com/v1

# Server Port (optional, defaults to 3000)
PORT=3000

